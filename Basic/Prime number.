n = int(input("Enter The Number:"))

if n > 1:
          for i in range(2,n):
          	if (n%i==0):
          		print(n, "is not a Prime Number")
          		break
          	else:
          		print(n, "is a Prime number")
# If the number is less than 1 it can't be Prime
else:
    print(n, "is not a Prime number")
