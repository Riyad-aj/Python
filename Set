num1= {1,2,3,4,4,5,6,7}
num2= set([4,5,7,8,9])
print(num1)
num2.add(6)
num2.remove(4)
print(num2)
print(5 in num2 )
print(num1 | num2)
print(num1 & num2)
print(num1 - num2)
