x = 37 ; y = 40 ; z = 8

print("Addition: ", x+y+z)
print("Subtraction: ", y- z)
print("Product:", x*y)
print("Quotient: ", y/z)

#Floor division to get result as integer( decimal point are removed).
print(x // z)

#Modulus operator divides one value by a second and gives the remainder as a result.
print(y% x)

a= 4
print("Exponent: ", a**3)
