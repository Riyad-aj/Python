# Check if the input number is odd or even.
num = int(input("Enter a number: "))

if (num % 2) == 0:
   print("{0} is Even".format(num))
else:
   print("{0} is Odd".format(num))
   
# Find even and odd number in the given range.
for i in range(25,31):
    if ((i % 2) == 0):
        print(i," is even")
    else:
        print(i," is odd")
