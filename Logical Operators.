#Returns True if both statements are true
x = 5

print(x > 4 and x < 10)
print(x < 7 and x > 10)

#Logical and returns the second value
print(10 and 20)

#Returns True if one of the statements is true
print(x < 7 or x > 10)

#Logical or returns the second value
print(10 or 20)

#Reverse the result, returns False if the result is true
print(not(x > 4 and x < 10))
print(not(x < 7 and x > 10))

#Logical not always return False for nonzero value
print(not 12)
print(not 0)
____________________________
